id:
sensor1: 0x010
sensor2: 0x011
command_sender: 0x001
    sends:
        0x01 {sensor_id} { high_byte 0 low_byte 0 one-shot high_byte 1 low_bite time period_ms high_byte 2 low_byte 0 off} trigger measurement
        0x02 {sensor_id} request last measurement
        0x03 change measurement unit
        0x04 ping sensor
        0x05 change i2c address
        0x06 request sensor status
sensor_manager: 0x002